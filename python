Arraydata = [10, 5, 6, 7, 1, 12, 13, 15, 21, 8]

def BubbleSort():
    max = 9
    swap = True
    while swap == True:
        swap = False
        for x in range(max):
            if Arraydata[x] < Arraydata[x+1]:
                temp = Arraydata[x]
                Arraydata[x] = Arraydata[x+1]
                Arraydata[x+1] = temp
                swap = True

BubbleSort()
for x in range(10):
        print(Arraydata[x])
